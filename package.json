{
	"name": "@solidbasisventures/intelliwaketsfoundation",
	"version": "5.1.14",
	"description": "NPM package for IntelliWake TypeScript Foundation Components",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"scripts": {
		"BuildAndPublish": "rm -rf dist/* && rollup -c && npm version patch --no-git-tag-version && npm publish",
		"BuildAndPublishMV": "rm -rf dist/* && rollup -c && mv dist/src/* dist/ && rm -rf dist/src && npm version patch --no-git-tag-version && npm publish",
		"YarnInstall": "yarn install",
		"YarnUpgrade": "yarn upgrade",
		"Test": "jest",
		"TSNode": "ts-node --script-mode consoles/consoles.ts",
		"TSNodeDev": "ts-node-dev --script-mode --respawn --transpile-only consoles/consoles.ts"
	},
	"files": [
		"dist"
	],
	"author": "SolidBasisVentures",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SolidBasisVentures/IntelliWakeTSFoundation.git"
	},
	"peerDependencies": {},
	"devDependencies": {
		"@types/jest": "^26.0.15",
		"@types/node": "^14.0.5",
		"babel-core": "^6.26.3",
		"babel-runtime": "^6.26.0",
		"jest": "^26.6.1",
		"prettier": "^2.0.5",
		"rollup": "^2.10.8",
		"rollup-plugin-terser": "^5.3.0",
		"rollup-plugin-typescript2": "^0.27.1",
		"rollup-plugin-flat-dts": "^2.0.0",
		"source-map-support": "^0.5.19",
		"ts-jest": "^26.4.3",
		"ts-node": "^9.0.0",
		"ts-node-dev": "^1.0.0",
		"typescript": "^4.0.5"
	}
}
